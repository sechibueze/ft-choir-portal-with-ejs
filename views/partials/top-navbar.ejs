<div class="row">
  <header>
    <div class="col-md-7">
      <nav class="navbar-default pull-left">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="offcanvas" data-target="#side-menu"
            aria-expanded="false">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>
      </nav>
      <!-- <div class="search hidden-xs hidden-sm">
        <input type="text" placeholder="Search" id="search">
      </div> -->
    </div>
    <div class="col-md-5">
      <div class="header-rightside">
        <ul class="list-inline header-top pull-right">

          <% if(data.download_url) {%>
          <a href="<%= data.download_url %>" target="_blank" class="btn btn-secondary">Download Report</a>
          <%}%>
          <!-- <li><a href="#"><i class="fa fa-envelope" aria-hidden="true"></i></a></li> -->
          <li>
            <a href="/dashboard?action=notifications&id=<%= data.user._id %>" class="icon-info">
          <i class="fa fa-bell" aria-hidden="true"></i>
          <span class="label label-primary"></span>
          </a>
          </li>
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown"><img src="<%= data.user.profileImage %>"
                style="width: 30px; height: 30px;" alt="user">
              <b class="caret"></b></a>
            <ul class="dropdown-menu">
              <li>
                <div class="navbar-content">
                  <span><%= data.user.firstname %> </span>
                  <p class="text-muted small">
                    <%= data.user.email %>
                  </p>
                  <div class="divider"></div>
                  <a href="/dashboard?action=profile&id=<%= data.user._id %>" class="view btn-sm active">View
                    Profile</a>

                </div>
              </li>
            </ul>
          </li>
        </ul>
      </div>
    </div>
  </header>
</div>